<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.wit.tourbooking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ie.Activities.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ie.Activities.Base" />
        <activity android:name="ie.Activities.Report" />
        <activity android:name="ie.Activities.History" />
        <activity android:name="ie.Activities.foodDrink" />
        <activity android:name="ie.Activities.Racing" />
        <activity android:name="ie.Activities.Visit" />
        <activity android:name="ie.Activities.Things" />
        <activity android:name="ie.Activities.Book" />
    </application>


</manifest>